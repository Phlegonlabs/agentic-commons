{
  "name": "agentic-commons",
  "version": "0.2.2",
  "description": "Unified token usage stats for AI coding tools",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/Phlegonlabs/agentic-commons",
  "bugs": {
    "url": "https://github.com/Phlegonlabs/agentic-commons/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Phlegonlabs/agentic-commons.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "files": [
    "apps/cli/dist",
    "README.md",
    "README.zh-CN.md",
    "README.zh-TW.md"
  ],
  "bin": {
    "acommons": "apps/cli/dist/cli.js"
  },
  "scripts": {
    "build": "npm run build -w @agentic-commons/shared && npm run build:cli",
    "build:cli": "tsc",
    "dev": "tsc --watch",
    "typecheck": "npm run build:cli && npm run typecheck -w @agentic-commons/shared"
  },
  "dependencies": {
    "chalk": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}
